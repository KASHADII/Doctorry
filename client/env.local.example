# Environment Variables for Doctorry Client
# Copy this file to .env.local and update the values

# API Configuration
VITE_API_URL=http://localhost:5000/api

# For production deployment, use your actual backend URL:
# VITE_API_URL=https://your-backend-url.onrender.com/api

# Google Translate API (if using translation features)
VITE_GOOGLE_TRANSLATE_API_KEY=your_api_key_here

# Default language
VITE_DEFAULT_SOURCE_LANGUAGE=auto

# Enable/disable Google Translate features
VITE_ENABLE_GOOGLE_TRANSLATE=true
